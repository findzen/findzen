package net.findzen.utils{    import flash.utils.describeType;    import flash.utils.getDefinitionByName;    import flash.utils.getQualifiedClassName;    import flash.utils.getQualifiedSuperclassName;    public class VarsBase    {        public function get classReference():Class        {            // override me            return Object;        }        public function toString():String        {            var ClassRef:Class = getDefinitionByName(getQualifiedClassName(this)) as Class;            var props:Object = Inspector.getProperties(this);            var propsList:String = '';            var i:String;            for(i in props)                propsList += '   ' + i + ': ' + this[i] + '\n';            return '\n' + ClassRef + ' :: [\n' + propsList + ' \n]';        }    }}