package net.findzen.mvc{	import net.findzen.mvc.*;	import net.findzen.events.CustomEvent;		import flash.events.EventDispatcher;	import flash.events.Event;	/**	 * Basic Model class in Model/View/Controller triad.	 */	public class Model extends EventDispatcher {		public static const UPDATE:String = "modelUpdated";		public function Model() { }		/**		 * This method will cause an update event and object		 * to be passed up to the view. Using CustomEvent allows		 * a data object to be passed along with the event		 */		public function onUpdate($ob:Object):void {			var e:CustomEvent = new CustomEvent(Model.UPDATE, true, false, $ob);			dispatchEvent(e);		}	}}