package net.findzen.mvc {	import net.findzen.mvc.*;	import flash.events.EventDispatcher;	/**	 * Minimum services that a "controller" class of	 * a Model/View/Controller triad must provide.	 */	public interface IController {		/**		 * Sets the model for this controller.		 */		function setModel(m:EventDispatcher):void;		/**		 * Returns the model for this controller.		 */		function getModel():EventDispatcher;		/**		 * Sets the view this controller is servicing.		 */		function setView(v:View):void;		/**		 * Returns this controller's view.		 */		function getView():View;	}}