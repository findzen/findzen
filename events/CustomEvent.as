package net.findzen.events {	    import flash.events.Event;		public class CustomEvent extends Event {				public var infoObj:Object;		        public function CustomEvent($type:String, $bubbles:Boolean, $cancelable:Boolean, $obj:Object) {            super($type, $bubbles, $cancelable);			infoObj = $obj;        }				public override function clone():Event {			return new CustomEvent(this.type, this.bubbles, this.cancelable, this.infoObj);		}				public override function toString():String {			return formatToString("CustomEvent", "type", "bubbles", "cancelable", "eventPhase", "infoObj");		}    }}